<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:id="@+id/main_container_333">
    <LinearLayout
        android:id="@+id/chart_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp">

        <TextView
            android:id="@+id/chart_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="start"
            android:text="@string/chart_title_color"
            android:textColor="@color/chart_title_color"
            android:layout_marginTop="16dp"
            android:textSize="16sp"/>

        <com.tarashor.chartlib.TelegramChart
            android:id="@+id/chart1"
            android:layout_width="match_parent"
            android:layout_height="450dp"
            android:layout_marginTop="8dp" />

        <com.tarashor.chartapp.view.LinesListView
            android:id="@+id/checkbox_list1"
            android:layout_marginTop="16dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" />

        <com.tarashor.chartlib.TelegramChart
            android:id="@+id/chart2"
            android:layout_width="match_parent"
            android:layout_height="450dp"
            android:layout_marginTop="24dp" />

        <com.tarashor.chartapp.view.LinesListView
            android:id="@+id/checkbox_list2"
            android:layout_marginTop="16dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" />

        <com.tarashor.chartlib.TelegramChart
            android:id="@+id/chart3"
            android:layout_width="match_parent"
            android:layout_height="450dp"
            android:layout_marginTop="24dp" />

        <com.tarashor.chartapp.view.LinesListView
            android:id="@+id/checkbox_list3"
            android:layout_marginTop="16dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" />

        <com.tarashor.chartlib.TelegramChart
            android:id="@+id/chart4"
            android:layout_width="match_parent"
            android:layout_height="450dp"
            android:layout_marginTop="24dp" />

        <com.tarashor.chartapp.view.LinesListView
            android:id="@+id/checkbox_list4"
            android:layout_marginTop="16dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" />

        <com.tarashor.chartlib.TelegramChart
            android:id="@+id/chart5"
            android:layout_width="match_parent"
            android:layout_height="450dp"
            android:layout_marginTop="24dp" />

        <com.tarashor.chartapp.view.LinesListView
            android:id="@+id/checkbox_list5"
            android:layout_marginTop="16dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" />

    </LinearLayout>

</android.support.v4.widget.NestedScrollView>
